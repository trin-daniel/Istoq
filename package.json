{
  "name": "istoq",
  "version": "1.0.0",
  "description": "sales management and control application",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "build": "rimraf dist && tsc -p build.json",
    "test": "jest --passWithNoTests --runInBand --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "Carlos Daniel",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.1",
    "rimraf": "^3.0.2",
    "sucrase": "^3.16.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "uuid": "^8.3.1",
    "validator": "^13.1.17"
  },
  "_moduleAliases": {
    "@infra": "dist/infra",
    "@main": "dist/main",
    "@data": "dist/data",
    "@domain": "dist/domain",
    "@validation": "dist/validation",
    "@presentation": "dist/presentation"
  },
  "engines": {
    "node": "12.x"
  }
}
